  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Debug File",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        // "run",
        "test",
        "--inspect-brk=127.0.0.1:9229",
        "--allow-env",
        "--allow-read=${workspaceFolder}",
        "--lock=lock_file.json",
        "--cached-only"
      ],
      "attachSimplePort": 9229
    }
  ]
}
