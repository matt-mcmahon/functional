{
  "author": "Matt McMahon <contact@matt-mcmahon.com> (http://matt-mcmahon.com/)",
  "contributors": [
    {
      "email": "contact@timdav.is",
      "files": [
        "./source/clone.js",
        "./source/cone.test.js"
      ],
      "name": "Tim Davis <thebearingedge>",
      "url": "https://www.npmjs.com/~thebearingedge"
    }
  ],
  "dependencies": {
    "@mwm/sign": "^1.3.2",
    "esm": "^3.2.25"
  },
  "description": "Extremely minimal functional library with a focus on source-code clarity.",
  "devDependencies": {
    "@mwm/describe": "^1.4.1",
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "prettier-check": "^2.0.0",
    "tap": "^14.9.2",
    "tap-nirvana": "^1.1.0",
    "typescript": "^3.7.2"
  },
  "esm": {
    "mode": "all",
    "sourceMap": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run silent"
    }
  },
  "keywords": [
    "functional",
    "experimental",
    "tutorial",
    "learning"
  ],
  "license": "MIT",
  "main": "main.cjs",
  "module": "module.js",
  "name": "@mwm/functional",
  "repository": {
    "type": "git",
    "url": "https://github.com/matt-mcmahon/functional.git"
  },
  "scripts": {
    "build": "tsc",
    "fix": "prettier --write source/**/*.js",
    "lint": "prettier-check source/**/*.js",
    "prepublishOnly": "npm run silent",
    "presilent": "tsc && prettier-check source/**/*.js",
    "pretest": "tsc && prettier-check source/**/*.js",
    "preversion": "npm run silent",
    "silent": "tap --bail -R silent ./source/**/*.test.js",
    "test": "tap -R spec ./source/**/*.test.js",
    "watch": "tap -R spec --watch ./source/**/*.test.js"
  },
  "type": "module",
  "types": "source/index.d.ts",
  "version": "5.3.0"
}
