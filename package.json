{
  "name": "@mwm/functional",
  "version": "2.0.0",
  "description": "Extremely minimal functional library with a focus on source-code clarity.",
  "main": "index.js",
  "module": "source/index.js",
  "scripts": {
    "prepublishOnly": "npm run test",
    "preversion": "npm run test",
    "lint": "prettier-check source/**/*.js",
    "pretest": "npm run lint",
    "test": "tape -r esm ./source/**/*.test.js | tap-spec"
  },
  "keywords": [
    "functional",
    "experimental",
    "tutorial",
    "learning"
  ],
  "author": "Matt McMahon <contact@matt-mcmahon.com> (http://matt-mcmahon.com/)",
  "contributors": [
    {
      "name": "Tim Davis <thebearingedge>",
      "email": "contact@timdav.is",
      "url": "https://www.npmjs.com/~thebearingedge",
      "files": [
        "./source/clone.js",
        "./source/cone.test.js"
      ]
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "husky": "^1.2.1",
    "prettier": "^1.15.3",
    "prettier-check": "^2.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matt-mcmahon/functional.git"
  },
  "dependencies": {
    "esm": "^3.0.74"
  },
  "esm": "auto",
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
